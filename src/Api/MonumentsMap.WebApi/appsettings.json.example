    },
    {
      "Name": "English",
      "Code": "en-GB"
    },
    {
      "Name": "Polski",
      "Code": "pl-PL"
    },
    {
      "Name": "Русский",
      "Code": "ru-RU"
    }
  ],
  "DefaultLanguage": "uk-UA",
  "SlugLanguage": "en-GB",
