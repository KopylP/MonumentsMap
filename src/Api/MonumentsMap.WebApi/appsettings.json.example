{
  "ConnectionStrings": {
    "DefaultConnection": "Host=db;Port=5432;Database=monumentsmap;Username=root;Password=root",
    "SqliteConnection": "Filename=./monuments.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
